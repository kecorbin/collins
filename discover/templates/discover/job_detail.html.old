{% extends "connect/base.html" %}

{% block content %}


<h1>Job #{{job.id}}</h1>
<h2>Job Type: {{job.type}}</h2>
<h2>Timestamp: {{job.created}}</h2>
<h2>Results</h2>




<div id="page-wrapper">

    <div class="container-fluid">


        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">



                    <div class="table-responsive">
                        <table id="example" class="table table-bordered" cellspacing="0" width="100%%">

                            <thead>
                            <tr>
                                <th>Host</th>
                                <th>Status</th>
                                <th>Open TCP Ports</th>

                            </tr>
                            </thead>


                            <tfoot>
                            <tr>
                                <th>Host</th>
                                <th>Status</th>
                                <th>Open TCP Ports</th>
                            </tr>

                            </tfoot>

                            <tbody>

                            {% for key, value in hosts.items %}

                            <tr>

                                <td>{{key}}</td>
                                <td>{{value.status.state}}</td>
                                <td>{% for port, value in value.tcp.items %}
                                    {{port}} : {{value.name}}<br>
                                    {% endfor %}
                                </td>
                            </tr>
                            {% endfor %}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>



{% endblock %}